{
    "presets": [
        "stage-0",
        "es2015",
        "stage-1",
        "es2016",
        "stage-2",
        "es2017",
        "stage-3"
    ],
    "plugins": [
        "transform-runtime",
        "transform-vue-jsx",
        "transform-decorators-legacy", ["component", [
            { "libraryName": "element-ui", "styleLibraryName": 'theme-chalk' }
        ]]
    ],
    "comments": false
}